CC=cc
CFLAGS=-Wall -Wextra -Werror
SRCS_DIR=srcs

all:
	$(CC) $(CFLAGS) -c $(SRCS_DIR)/ft_putchar.c \
	       	-c $(SRCS_DIR)/ft_swap.c \
		-c $(SRCS_DIR)/ft_putstr.c \
		-c $(SRCS_DIR)/ft_strlen.c \
		-c $(SRCS_DIR)/ft_strcmp.c
